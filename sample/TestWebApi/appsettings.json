{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Services": {
    "Logging": true,
    "Hsts": {
      "ExcludedHosts": {
        "Clear": true
      },
      "Preload": true,
      "IncludeSubDomains": true,
      "MaxAge": "356.00:00:00"
    },
    "Configure<Microsoft.AspNetCore.Builder.CookiePolicyOptions>": {
      "HttpOnly": "Always",
      "Secure": "Always"
    },
    "Controllers": true,
    "EndpointsApiExplorer": true,
    "SwaggerGen": true
  }
}
